
@model IList<TPHS.Models.AnimalSpeciesType>

<h1>@ViewBag.Title</h1>
<table class="table">
       <tr>
           <th>The Diversity</th>
       </tr>
       @foreach (var AnimalType in Model)
       {
           <tr>
               <td>@AnimalType.Name</td>
           </tr>
       }
</table>   
        

<p><a asp-controller="AnimalType" asp-action="Add">Add Animal Breed or Species</a></p>
<p><a asp-controller="AnimalType" asp-action="Remove">Remove Animal Breed or Species</a></p>
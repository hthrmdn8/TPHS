@model AnimalSpeciesType
<h1>@ViewBag.title</h1>


<form method="post">
    @foreach (var animalType in ViewBag.animalTypes)
    {
        <input type="checkbox" name="animalIds" id="@animalType.ID" value="@animalType.ID" />
        <label for="@animalType.ID">@animalType.Name</label>
        <br />
    }
    <input type="submit" value="Remove Selected Animal Breed(s) or Type(s)" />
</form>